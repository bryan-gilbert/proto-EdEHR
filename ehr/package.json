{
  "name": "ed-ehr",
  "version": "1.0.0",
  "description": "The EHR of the proof of concept EdEHR",
  "author": "Bryan Gilbert",
  "license": "MIT",
  "scripts": {
    "start": "npm run start-dev",
    "start-dev": "NODE_ENV=development PORT=28000 node dev-server-wp-dev.js",
    "start-prod": "NODE_ENV=production http-server dist -p 28000",
    "build-dev": "NODE_ENV=development webpack --progress --hide-modules --config webpack.dev.js",
    "build": "NODE_ENV=production webpack --progress --hide-modules --config webpack.prod.js",
    "pm2-prod": "NODE_ENV=production npm run build ; pm2 start pm2-prod.json",
    "temp-test": "nyc mocha --exit",
    "temp-dev": "NODE_ENV=development webpack-dev-server --open --hot",
    "temp-start-0": "npm run start-server | npm run dev",
    "install-all": "npm install",
    "install-http-server": "npm install http-server -g",
    "start:server": "rm -rf ./build && webpack --config webpack.server.config.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "http-server": "^0.11.1",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-loader": "^8.0.4",
    "bulma": "^0.7.1",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.2",
    "mocha": "^5.2.0",
    "node-sass": "^4.5.3",
    "node-sass-magic-importer": "^5.2.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "prettier": "^1.14.3",
    "sass-loader": "^7.1.0",
    "start-server-webpack-plugin": "^2.2.5",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "vue-loader": "^15.4.2",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2",
    "webpack-serve": "^2.0.2"
  }
}
