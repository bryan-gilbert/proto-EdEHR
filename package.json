    "camelcase": "^5.0.0",
    "chalk": "^2.4.1",
    "coveralls": "^3.0.2",
    "nyc": "^13.0.1",
    "should": "^13.2.3",
    "standard": "^10.0.0",
    "supertest": "^3.3.0"
  }
