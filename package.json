  "scripts": {
    "start": "npm run serve:dev-wp",
    "serve:dev-wp": "NODE_ENV=development node bin/dev-server-wp-dev.js",
    "dev": "NODE_ENV=development webpack-dev-server --open --hot",
    "start-0": "npm run start-server | npm run dev",
