    "test": "nyc mocha --exit",
    "install-all": "npm install | npm run install-db",
    "install-db": "cd dbjson; npm run install-all"
  },
  "dependencies": {
    "ajv": "^6.5.4"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
